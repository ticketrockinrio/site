<div class="main_content event_5149 channel_21649">


		<div class="event_container">
			<div id="order_bar_container" class="order-top" style="display:none;"></div>
			<div id="notification_modal"> </div>
			<div id="detail_container" class="detail-container">
				<div id="event_content" >
						<div id="event_header" class="event-header" >
							<div id="eventCover">
								<img class="hidden-xs" src="https://cdn.getcrowder.com/images/bf37edc6-5a3e-43c7-8b81-0787e5d223f5-banner-home-site-tmvendasabertas15-05nr-2.png" alt="Rock in Rio" width="100%">
								<img class="visible-xs" src="https://cdn.getcrowder.com/images/bf37edc6-5a3e-43c7-8b81-0787e5d223f5-banner-home-site-tmvendasabertas15-05nr-2.png" alt="Rock in Rio" width="100%">
